{
    "name": "@panorama-viewer/react-client",
    "version": "1.0.1",
    "license": "MIT",
    "author": {
        "name": "Serhii Hrynko"
    },
    "scripts": {
        "start": "node scripts/start.js",
        "build": "node scripts/build.js",
        "test": "node scripts/test.js --env=jsdom"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions"
    ],
    "dependencies": {
        "react": "^16.1.1",
        "react-dev-utils": "^4.2.1",
        "react-dom": "^16.1.1"
    },
    "devDependencies": {
        "@panorama-viewer/model": "file:../model",
        "@panorama-viewer/client": "file:../client",
        "@types/jest": "^21.1.8",
        "@types/node": "^8.0.53",
        "@types/react": "^16.0.25",
        "@types/react-dom": "^16.0.3",
        "autoprefixer": "7.1.2",
        "case-sensitive-paths-webpack-plugin": "2.1.1",
        "chalk": "1.1.3",
        "sass-loader": "^6.0.6",
        "node-sass": "^4.7.2",
        "css-loader": "0.28.4",
        "dotenv": "4.0.0",
        "extract-text-webpack-plugin": "3.0.0",
        "file-loader": "0.11.2",
        "fs-extra": "3.0.1",
        "html-webpack-plugin": "2.29.0",
        "jest": "^21.2.1",
        "postcss-flexbugs-fixes": "3.2.0",
        "postcss-loader": "2.0.6",
        "source-map-loader": "^0.2.1",
        "style-loader": "^0.18.2",
        "sw-precache-webpack-plugin": "^0.11.4",
        "ts-jest": "^20.0.7",
        "ts-loader": "^2.3.7",
        "tslint": "^5.8.0",
        "tslint-loader": "^3.5.3",
        "tslint-react": "^3.2.0",
        "typescript": "^2.6.1",
        "url-loader": "^0.5.9",
        "webpack": "^3.8.1",
        "webpack-dev-server": "^2.9.4",
        "webpack-manifest-plugin": "^1.3.2"
    },
    "jest": {
        "mapCoverage": true,
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,ts,tsx}"
        ],
        "setupFiles": [
            "<rootDir>/config/polyfills.js"
        ],
        "testMatch": [
            "<rootDir>/src/**/__tests__/**/*.ts?(x)",
            "<rootDir>/src/**/?(*.)(spec|test).ts?(x)"
        ],
        "testEnvironment": "node",
        "testURL": "http://localhost",
        "transform": {
            "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
            "^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
            "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
        },
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$"
        ],
        "moduleNameMapper": {
            "^react-native$": "react-native-web"
        },
        "moduleFileExtensions": [
            "web.ts",
            "ts",
            "web.tsx",
            "tsx",
            "web.js",
            "js",
            "web.jsx",
            "jsx",
            "json",
            "node"
        ],
        "globals": {
            "ts-jest": {
                "tsConfigFile": "D:\\PanoramaViewer\\repo\\react\\tsconfig.test.json"
            }
        }
    },
    "eslintConfig": {
        "extends": "react-app"
    }
}
